{
    "manifest_version": 3,
    "name": "__MSG_appName__",
    "version": "7.1",
    "description": "__MSG_appDesc__",
    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png"
    },
    "homepage_url": "https://github.com/BirdInFire/Aelisya-Tweaker",
    "default_locale": "en",
    "content_security_policy": {
        "extension_pages": "default-src 'self'",
        "isolated_world": "default-src 'self'"
    },
    "permissions": [
        "declarativeNetRequest"
    ],
    "declarative_net_request": {
        "rule_resources": [{
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules.json"
        }]
    },
    "action": {
        "default_icon": {
            "48": "icons/icon48.png"
        },
        "default_popup": "pages/popup.html"
    }
}
